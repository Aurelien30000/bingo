data merge storage neun_einser.timer:display {internal:{secondSeperator: ["."], millis: '[{"storage": "neun_einser.timer:display", "nbt": "internal.secondSeperator", "interpret": true}, {"score":{"name": "$millis", "objective": "91.timer.time"}}]'}}
